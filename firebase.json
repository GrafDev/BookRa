{
  "hosting": [
    {
      "site": "bookra-scratch-click",
      "public": "dist/scratch-click",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "site": "bookra-scratch-auto",
      "public": "dist/scratch-auto",
      "ignore": [
        "firebase.json",
